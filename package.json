{
  "name": "multi-git",
  "version": "2.3.0",
  "description": "Handle multiple git repositories at once",
  "bin": {
    "multi-git": "./bin/multi-git.js"
  },
  "main": "./build/lib.js",
  "engines": {
    "node": "6.8.0"
  },
  "scripts": {
    "build": "babel src -d build -s",
    "start": "npm run build -- -w",
    "install": "npm run build && node npm-scripts.js install",
    "test": "mocha --compilers js:babel-core/register",
    "lint": "eslint src bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolas-schmitt/multi-git.git"
  },
  "bugs": {
    "url": "https://github.com/nicolas-schmitt/multi-git/issues"
  },
  "homepage": "https://github.com/nicolas-schmitt/multi-git#multi-git",
  "author": "Nicolas Schmitt <https://github.com/nicolas-schmitt>",
  "license": "MIT",
  "keywords": [
    "git",
    "multi",
    "multiple",
    "micro",
    "service"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "bluebird": "^3.5.0",
    "bump-regex": "^2.7.0",
    "cli-table2": "^0.2.0",
    "colors": "^1.1.2",
    "inquirer-confirm": "^0.2.2",
    "lodash": "^4.17.4",
    "simple-git": "^1.73.0",
    "source-map-support": "^0.4.15",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^0.7.1",
    "chai-string": "^1.4.0",
    "eslint": "^4.1.1",
    "mocha": "^3.4.2",
    "mock-fs": "^4.00.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0"
  },
  "babel": {
    "presets": [
      ["env", {
          "targets": {
            "node": "current"
          }
      }]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
