{
  "name": "multi-git",
  "version": "2.0.0",
  "description": "Handle multiple git repositories at once",
  "bin": {
    "multi-git": "./bin/multi-git.js"
  },
  "main": "./src/lib.js",
  "engines": {
    "node": "6.8.0"
  },
  "scripts": {
    "build": "babel src -d build -s",
    "watch": "npm run build -- -w",
    "install": "npm run build && node npm-scripts.js install",
    "test": "mocha",
    "lint": "eslint src bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolas-schmitt/multi-git.git"
  },
  "bugs": {
    "url": "https://github.com/nicolas-schmitt/multi-git/issues"
  },
  "homepage": "https://github.com/nicolas-schmitt/multi-git#multi-git",
  "author": "Nicolas Schmitt <https://github.com/nicolas-schmitt>",
  "license": "MIT",
  "keywords": [
    "git",
    "multi",
    "multiple",
    "micro",
    "service"
  ],
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "0.0.9",
    "bluebird": "^3.4.6",
    "bump-regex": "^2.3.0",
    "cli-table2": "^0.2.0",
    "colors": "^1.1.2",
    "inquirer-confirm": "^0.2.2",
    "lodash": "^4.16.4",
    "simple-git": "^1.63.0",
    "source-map-support": "^0.4.6",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "chai-string": "^1.2.0",
    "eslint": "^3.7.1",
    "mocha": "^3.1.2",
    "mock-fs": "^3.11.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.10"
  },
  "babel": {
    "presets": [
      ["env", {"targets": {"node": "current"}}]
    ]
  }
}
